	component WRAM is
		port (
			data    : in  std_logic_vector(287 downto 0) := (others => 'X'); -- datain
			q       : out std_logic_vector(287 downto 0);                    -- dataout
			address : in  std_logic_vector(3 downto 0)   := (others => 'X'); -- address
			wren    : in  std_logic                      := 'X';             -- wren
			clock   : in  std_logic                      := 'X'              -- clk
		);
	end component WRAM;

